<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <Nullable>disable</Nullable>
        <ApplicationIcon>Properties\icon.ico</ApplicationIcon>
        <PlatformTarget>x64</PlatformTarget>
        <OutputType>Exe</OutputType>
    </PropertyGroup>

    <ItemGroup>
      <Content Include="Properties\icon.ico" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="6.0.0" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="6.0.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
        <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
        <PackageReference Include="Microsoft.Data.Sqlite" Version="6.0.4" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\BookmarksBase.Storage\BookmarksBase.Storage.csproj">
        </ProjectReference>
    </ItemGroup>

    <ItemGroup>
        <None Update="appsettings.importer.json">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
    </ItemGroup>

    <ItemGroup>
        <None Include=".editorconfig" />

        <None Include="lynx\lynx.cfg">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>

        <None Include="lynx\lynx.exe">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>

        <None Include="lynx\libcrypto-1_1.dll">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>

        <None Include="lynx\libssl-1_1.dll">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>

    </ItemGroup>

</Project>



